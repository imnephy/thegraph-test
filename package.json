{
  "name": "unistory-vite-template",
  "version": "0.0.1",
  "author": "Unistory",
  "repository": {
    "type": "git",
    "url": ""
  },
  "private": "true",
  "scripts": {
    "dev": "vite --host",
    "build:development": "tsc && vite build --mode development",
    "build:staging": "tsc && vite build --mode staging",
    "build:production": "tsc && vite build --mode production",
    "preview": "vite preview",
    "https-preview": "serve dist",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:ci": "eslint src --ext .ts,.tsx -f checkstyle --output-file eslint.xml",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "check-types": "tsc --pretty --skipLibCheck --noEmit",
    "test": "jest --maxWorkers=50%",
    "test:ci": "jest --ci --coverage --runInBand",
    "commit": "git-cz",
    "generate-images": "gulp responsive_img",
    "ac": "npm run check-types && npm run lint:fix && git add . && npm run commit"
  },
  "dependencies": {
    "graphql": "^16.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-helmet-async": "^1.3.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.15.0",
    "urql": "^4.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.22.15",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/cz-commitlint": "^17.7.1",
    "@svgr/webpack": "^8.1.0",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "cz-git": "^1.7.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-boundaries": "^3.4.0",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-testing-library": "^6.0.1",
    "eslint-plugin-unicorn": "^48.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "gulp": "^4.0.2",
    "gulp-sharp-responsive": "^0.4.1",
    "https-localhost": "^4.7.1",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "jest-junit": "^16.0.0",
    "jest-transformer-svg": "^2.0.1",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.31",
    "prettier": "3.0.3",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-pwa": "^0.16.4",
    "vite-plugin-svgr": "^3.2.0",
    "whatwg-fetch": "^3.6.18"
  },
  "config": {
    "commitizen": {
      "path": "cz-git",
      "czConfig": "commitlint.config.js"
    }
  }
}
